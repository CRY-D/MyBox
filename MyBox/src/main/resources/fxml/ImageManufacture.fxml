<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="750.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImageManufactureController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" minHeight="100.0" VBox.vgrow="NEVER">
                    <children>
                        <TabPane fx:id="tabPane" maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" tabClosingPolicy="UNAVAILABLE" HBox.hgrow="ALWAYS">
                            <tabs>
                                <Tab fx:id="fileTab" text="%File">
                                    <content>
                                        <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308">
                                            <children>
                                                <ToolBar fx:id="fileBar" nodeOrientation="RIGHT_TO_LEFT" HBox.hgrow="ALWAYS">
                                                    <items>
                                                        <Button mnemonicParsing="false" onAction="#saveAs" text="%SaveAs" />
                                                        <Button fx:id="iButton" mnemonicParsing="false" onAction="#popImageInformation" text="%ImageInformation" />
                                                        <Button fx:id="mButton" mnemonicParsing="false" onAction="#popMetaData" text="%ImageMetaData" />
                                                        <CheckBox fx:id="saveCheck" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" selected="true" text="%ConfirmWhenSave" />
                                                        <CheckBox fx:id="openCheck" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" selected="true" text="%OpenAfterSaveAs" />
                                                    </items>
                                                </ToolBar>
                                                <ToolBar nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="NEVER">
                                                    <items>
                                                        <Button mnemonicParsing="false" onAction="#selectSourceFile" text="%Select..." />
                                                    </items>
                                                </ToolBar>
                                            </children>
                                        </HBox>
                                    </content>
                                </Tab>
                                <Tab fx:id="zoomTab" text="%View">
                                    <content>
                                        <ToolBar maxWidth="1.7976931348623157E308">
                                            <items>
                                                <Label fx:id="zoomValue" text="10%" />
                                                <Slider fx:id="zoomSlider" blockIncrement="5.0" max="95.0" min="5.0" showTickLabels="true" showTickMarks="true" value="10.0" />
                                                <Label text="%ZoomStep" />
                                                <Button fx:id="mrButton" mnemonicParsing="false" onAction="#moveRight" text="%MoveRight" />
                                                <Button fx:id="mlButton" mnemonicParsing="false" onAction="#moveLeft" text="%MoveLeft" />
                                                <Button fx:id="upButton" mnemonicParsing="false" onAction="#moveUp" text="%MoveUp" />
                                                <Button fx:id="downButton" mnemonicParsing="false" onAction="#moveDown" text="%MoveDown" />
                                                <Button fx:id="rButton" mnemonicParsing="false" onAction="#rotateRight" text="%RotateRight" />
                                                <Button fx:id="lButton" mnemonicParsing="false" onAction="#rotateLeft" text="%RotateLeft" />
                                                <Button fx:id="tButton" mnemonicParsing="false" onAction="#turnOver" text="%TurnOver" />
                                                <Button mnemonicParsing="false" onAction="#straighten" text="%Straighten" />
                                                <Label fx:id="rotateValue" text="90" />
                                                <Slider fx:id="rotateSlider" blockIncrement="5.0" max="355.0" min="5.0" prefHeight="44.0" prefWidth="150.0" showTickLabels="true" showTickMarks="true" value="90.0" />
                                                <Label text="%RotateStep" />
                                            </items>
                                        </ToolBar>
                                    </content>
                                </Tab>
                                <Tab fx:id="browseTab" text="%Browse">
                                    <content>
                                        <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT">
                                            <children>
                                                <ToolBar HBox.hgrow="NEVER">
                                                    <items>
                                                        <Button mnemonicParsing="false" onAction="#browseAction" text="%MultipleImagesViewer" />
                                                    </items>
                                                </ToolBar>
                                                <ToolBar fx:id="navBar" maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" HBox.hgrow="ALWAYS">
                                                    <items>
                                                        <Button fx:id="nextButton" mnemonicParsing="false" onAction="#next" text="%NextImage" />
                                                        <Button fx:id="lastButton" mnemonicParsing="false" onAction="#last" text="%PreviousImage" />
                                                        <RadioButton mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%Size">
                                                            <toggleGroup>
                                                                <ToggleGroup fx:id="sortGroup" />
                                                            </toggleGroup>
                                                        </RadioButton>
                                                        <RadioButton mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%ModifyTime" toggleGroup="$sortGroup" />
                                                        <RadioButton mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" selected="true" text="%FileName" toggleGroup="$sortGroup" />
                                                        <Label text="%Sort" />
                                                    </items>
                                                </ToolBar>
                                            </children>
                                        </HBox>
                                    </content>
                                </Tab>
                                <Tab fx:id="pixelsTab" text="%Pixels">
                                    <content>
                                        <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308">
                                            <children>
                                                <ToolBar HBox.hgrow="NEVER">
                                                    <items>
                                                        <Button fx:id="pixelsOkButton" mnemonicParsing="false" onAction="#pixelsAction" text="%OK" />
                                                    </items>
                                                </ToolBar>
                                                <ToolBar fx:id="scaleBar" disable="true" nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="NEVER">
                                                    <items>
                                                        <Label text="%Width" />
                                                        <TextField fx:id="widthInput" prefWidth="80.0" />
                                                        <Label text="%Height" />
                                                        <TextField fx:id="heightInput" prefWidth="80.0" />
                                                        <CheckBox fx:id="keepRatioCheck" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" selected="true" text="%KeepRatio" />
                                                        <ChoiceBox fx:id="ratioBox" prefWidth="140.0" />
                                                        <Button mnemonicParsing="false" onAction="#setOriginalSize" text="%OriginalSize" />
                                                        <Button mnemonicParsing="false" onAction="#pixelsCalculator" text="%PixelsCalculator" />
                                                    </items>
                                                </ToolBar>
                                                <ToolBar maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="NEVER">
                                                    <items>
                                                        <RadioButton contentDisplay="TOP" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" selected="true" text="%SampleScale">
                                                            <toggleGroup>
                                                                <ToggleGroup fx:id="pixelsGroup" />
                                                            </toggleGroup></RadioButton>
                                                        <TextField fx:id="scaleInput" prefWidth="50.0" text="1.0" />
                                                        <RadioButton mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%Pixels" toggleGroup="$pixelsGroup">
                                             <padding>
                                                <Insets left="10.0" />
                                             </padding>
                                                        </RadioButton>
                                                    </items>
                                                </ToolBar>
                                    <ToolBar HBox.hgrow="ALWAYS">
                                      <items>
                                        <Button mnemonicParsing="false" text=" " visible="false" />
                                      </items>
                                    </ToolBar>
                                            </children>
                                        </HBox>
                                    </content>
                                </Tab>
                                <Tab fx:id="hueTab" text="%Hue">
                                    <content>
                                        <ToolBar maxWidth="1.7976931348623157E308">
                                            <items>
                                                <Button mnemonicParsing="false" onAction="#increaseHue" text="%Increase" />
                                                <Button mnemonicParsing="false" onAction="#decreaseHue" text="%Decrease" />
                                                <Label fx:id="hueValue" text="5">
                                                    <padding>
                                                        <Insets left="40.0" />
                                                    </padding>
                                                </Label>
                                                <Slider fx:id="hueSlider" blockIncrement="5.0" max="359.0" min="1.0" prefHeight="44.0" prefWidth="150.0" showTickLabels="true" showTickMarks="true" value="5.0" />
                                                <Label text="%AdjustmentStep" />
                                            </items>
                                        </ToolBar>
                                    </content>
                                </Tab>
                                <Tab fx:id="saturateTab" text="%Saturation">
                                    <content>
                                        <ToolBar maxWidth="1.7976931348623157E308">
                                            <items>
                                                <Button mnemonicParsing="false" onAction="#increaseSaturate" text="%Increase" />
                                                <Button mnemonicParsing="false" onAction="#decreaseSaturate" text="%Decrease" />
                                                <Label fx:id="saturateValue" text="5%">
                                                    <padding>
                                                        <Insets left="40.0" />
                                                    </padding>
                                                </Label>
                                                <Slider fx:id="saturateSlider" blockIncrement="5.0" max="95.0" min="5.0" showTickLabels="true" showTickMarks="true" value="5.0" />
                                                <Label text="%AdjustmentStep" />
                                            </items>
                                        </ToolBar>
                                    </content>
                                </Tab>
                                <Tab fx:id="brightnessTab" text="%Brightness">
                                    <content>
                                        <ToolBar maxWidth="1.7976931348623157E308">
                                            <items>
                                                <Button mnemonicParsing="false" onAction="#increaseBrightness" text="%Increase" />
                                                <Button mnemonicParsing="false" onAction="#decreaseBrightness" text="%Decrease" />
                                                <Label fx:id="brightnessValue" text="5%">
                                                    <padding>
                                                        <Insets left="40.0" />
                                                    </padding>
                                                </Label>
                                                <Slider fx:id="brightnessSlider" blockIncrement="5.0" max="95.0" min="5.0" showTickLabels="true" showTickMarks="true" value="5.0" />
                                                <Label text="%AdjustmentStep" />
                                            </items>
                                        </ToolBar>
                                    </content>
                                </Tab>
                                <Tab fx:id="filtersTab" text="%Filters">
                                    <content>
                                        <ToolBar maxWidth="1.7976931348623157E308">
                                            <items>
                                                <Button mnemonicParsing="false" onAction="#setInvert" text="%InvertColor" />
                                                <Button mnemonicParsing="false" onAction="#setGray" text="%Gray" />
                                    <Label text="     " />
                                                <Button mnemonicParsing="false" onAction="#setBinary" text="%BlackOrWhite" />
                                                <Button mnemonicParsing="false" onAction="#calculateThreshold" text="%CalculateThreshold" />
                                                <Label fx:id="binaryValue" text="50%" />
                                                <Slider fx:id="binarySlider" blockIncrement="5.0" max="95.0" min="5.0" showTickLabels="true" showTickMarks="true" value="50.0" />
                                                <Label text="%BinaryThreshold" />
                                            </items>
                                        </ToolBar>
                                    </content>
                                </Tab>
                                <Tab fx:id="replaceColorTab" text="%ReplaceColor">
                           <content>
                              <HBox nodeOrientation="LEFT_TO_RIGHT">
                                 <children>
                                    <ToolBar prefWidth="10.0" HBox.hgrow="ALWAYS">
                                      <items>
                                        <Button mnemonicParsing="false" text="  " visible="false" />
                                      </items>
                                    </ToolBar>
                                              <ToolBar maxWidth="1.7976931348623157E308" HBox.hgrow="NEVER">
                                                  <items>
                                          <RadioButton mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" selected="true" text="%AccurateMatch">
                                             <toggleGroup>
                                                <ToggleGroup fx:id="colorMatchGroup" />
                                             </toggleGroup>
                                          </RadioButton>
                                          <RadioButton mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%ColorDistance" toggleGroup="$colorMatchGroup" />
                                          <TextField fx:id="colorDistanceInput" disable="true" prefHeight="30.0" prefWidth="67.0" promptText="0~255" />
                                          <Label text="%NewColor">
                                             <padding>
                                                <Insets left="40.0" />
                                             </padding>
                                          </Label>
                                          <ColorPicker fx:id="newColorPicker" />
                                          <Button mnemonicParsing="false" onAction="#pickColorForNew" text="%PickColorOnImage" />
                                          <Label text="%OriginalColor">
                                             <padding>
                                                <Insets left="40.0" />
                                             </padding></Label>
                                          <ColorPicker fx:id="originalColorPicker" />
                                                      <Button mnemonicParsing="false" onAction="#pickColorForOriginal" text="%PickColorOnImage" />
                                          <Label prefWidth="10.0" text=" " />
                                          <Button fx:id="colorOkButton" mnemonicParsing="false" onAction="#replaceColorAction" text="%OK" />
                                                  </items>
                                              </ToolBar>
                                 </children>
                              </HBox>
                           </content>
                                </Tab>
                                <Tab fx:id="refTab" text="%Reference">
                                    <content>
                                        <HBox alignment="CENTER_LEFT">
                                            <children>
                                                <ToolBar fx:id="refBar" disable="true" HBox.hgrow="NEVER">
                                                    <items>
                                                        <Button fx:id="selectRefButton" mnemonicParsing="false" onAction="#selectReference" text="%Select..." />
                                                        <Button fx:id="origImageButton" mnemonicParsing="false" onAction="#originalImage" text="%OriginalImage" />
                                                        <Button mnemonicParsing="false" onAction="#popRefInformation" text="%ImageInformation" />
                                                        <Button mnemonicParsing="false" onAction="#popRefMeta" text="%ImageMetaData" />
                                                        <CheckBox fx:id="refSyncCheck" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%SychronizeViewOriginal" />
                                                    </items>
                                                </ToolBar>
                                                <ToolBar prefHeight="40.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                                    <items>
                                                        <Button mnemonicParsing="false" prefWidth="1.0" visible="false" />
                                                    </items>
                                                </ToolBar>
                                            </children>
                                        </HBox>
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT">
                    <children>
                        <ToolBar HBox.hgrow="NEVER">
                            <items>
                                <Label fx:id="tipsLabel" prefHeight="20.0" text="%Tips" />
                                <Button mnemonicParsing="false" visible="false" />
                            </items>
                        </ToolBar>
                        <ToolBar fx:id="hotBar" disable="true" maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" HBox.hgrow="ALWAYS">
                            <items>
                                <Button fx:id="saveButton" disable="true" mnemonicParsing="false" onAction="#save" text="%Save" />
                                <Button fx:id="recoverButton" disable="true" mnemonicParsing="false" onAction="#recovery" text="%Recover" />
                                <Button fx:id="oButton" mnemonicParsing="false" onAction="#imageSize" text="%ImageSize" />
                                <Button fx:id="wButton" mnemonicParsing="false" onAction="#paneSize" text="%PaneSize" />
                                <Button fx:id="inButton" mnemonicParsing="false" onAction="#zoomIn" text="%ZoomIn" />
                                <Button fx:id="outButton" mnemonicParsing="false" onAction="#zoomOut" text="%ZoomOut" />
                                <CheckBox fx:id="displayRefCheck" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%DisplayReference" />
                            </items>
                        </ToolBar>
                    </children>
                </HBox>
                <SplitPane fx:id="splitPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                    <items>
                        <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true">
                            <content>
                                <ImageView fx:id="imageView" cache="true" fitHeight="546.0" fitWidth="1067.0" onMouseClicked="#clickImage" onMouseEntered="#showLabel" pickOnBounds="true" preserveRatio="true">
                                    <cursor>
                                        <Cursor fx:constant="OPEN_HAND" />
                                    </cursor>
                                </ImageView>
                            </content>
                        </ScrollPane>
                    </items>
                </SplitPane>
            </children>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <padding>
                <Insets bottom="5.0" top="5.0" />
            </padding>
        </VBox>
    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <bottom>
        <Label fx:id="bottomLabel" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER_LEFT" />
    </bottom>
</BorderPane>
