<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="750.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImageGifEditerController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox fx:id="mainPane" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0" BorderPane.alignment="CENTER">
            <children>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
              <items>
                <Button mnemonicParsing="false" onAction="#createAction" text="%Create" />
                  <Button mnemonicParsing="false" onAction="#openAction" text="%Open" />
              </items>
            </ToolBar>
                <SplitPane fx:id="splitPane" dividerPositions="0.278391167192429" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                    <items>
                        <VBox fx:id="valuesBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <children>
                                <ToolBar maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
                                    <items>
                                        <Button fx:id="addButton" mnemonicParsing="false" onAction="#addAction" text="%Add" />
                                        <Button fx:id="upButton" mnemonicParsing="false" onAction="#upAction" text="%Up" />
                                        <Button fx:id="downButton" mnemonicParsing="false" onAction="#downAction" text="%Down" />
                                        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteAction" text="%Delete" />
                                        <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clearAction" text="%Clear" />
                                    </items>
                                </ToolBar>
                                <TableView fx:id="sourceTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                                    <columns>
                                        <TableColumn fx:id="imageColumn" editable="false" prefWidth="120.0" text="%SourceImage" />
                                        <TableColumn fx:id="sizeColumn" prefWidth="100.0" text="%Size" />
                                        <TableColumn fx:id="fileColumn" prefWidth="500.0" text="%File" />
                                    </columns>
                                </TableView>
                            </children>
                        </VBox>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0">
                            <children>
                                <TitledPane maxWidth="1.7976931348623157E308" text="%Options" VBox.vgrow="NEVER">
                                    <content>
                                        <VBox spacing="10.0">
                                            <children>
                                                <HBox alignment="CENTER_LEFT" spacing="10.0">
                                                    <children>
                                          <Label text="%IntervalMilliseconds" />
                                                        <ComboBox fx:id="intervalCBox" editable="true" prefWidth="100.0" />
                                          <CheckBox fx:id="loopCheck" mnemonicParsing="false" selected="true" text="%Loop" />
                                                    </children>
                                                </HBox>
                                                <HBox alignment="CENTER_LEFT" spacing="10.0">
                                       <children>
                                          <RadioButton mnemonicParsing="false" selected="true" text="%KeepImagesSize">
                                             <toggleGroup>
                                                <ToggleGroup fx:id="sizeGroup" />
                                             </toggleGroup>
                                          </RadioButton>
                                          <RadioButton mnemonicParsing="false" text="%AllSetAs" toggleGroup="$sizeGroup" />
                                          <TextField fx:id="widthInput" prefWidth="100.0" promptText="%Width" />
                                          <TextField fx:id="heightInput" prefWidth="100.0" promptText="%Height" />
                                       </children></HBox>
                                            </children>
                                        </VBox>
                                    </content>
                                </TitledPane>
                                <HBox fx:id="toolBox" alignment="CENTER_LEFT" VBox.vgrow="NEVER">
                                    <children>
                                        <Label maxWidth="1.7976931348623157E308" styleClass="bold-blue-label" stylesheets="@../styles/MyBox.css" HBox.hgrow="ALWAYS">
                                            <padding>
                                                <Insets left="20.0" />
                                            </padding>
                                        </Label>
                                        <HBox alignment="CENTER_RIGHT" spacing="10.0">
                                            <children>
                                    <Button fx:id="stopButton" mnemonicParsing="false" onAction="#stopAction" text="%Stop" />
                                                <Button fx:id="zoomOutButton" mnemonicParsing="false" onAction="#zoomOut" text="%ZoomOut" />
                                                <Button fx:id="zoomInButton" mnemonicParsing="false" onAction="#zoomIn" text="%ZoomIn" />
                                                <Button fx:id="paneSizeButton" mnemonicParsing="false" onAction="#paneSize" text="%PaneSize" />
                                                <Button fx:id="imageSizeButton" mnemonicParsing="false" onAction="#imageSize" text="%ImageSize" />
                                    <CheckBox fx:id="viewCheck" mnemonicParsing="false" selected="true" text="%ViewAfterSave" />
                                    <Button fx:id="saveAsButton" mnemonicParsing="false" onAction="#saveAsAction" text="%SaveAs" />
                                                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveAction" text="%Save" />
                                            </children>
                                        </HBox>
                                    </children>
                                </HBox>
                                <ScrollPane fx:id="scrollPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true" VBox.vgrow="ALWAYS">
                                    <content>
                                        <ImageView fx:id="imageView" fitHeight="261.0" fitWidth="841.0" pickOnBounds="true" preserveRatio="true">
                                            <cursor>
                                                <Cursor fx:constant="OPEN_HAND" />
                                            </cursor>
                                        </ImageView>
                                    </content>
                                </ScrollPane>
                            </children>
                        </VBox>
                    </items>
                </SplitPane>
            </children>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <bottom>
        <Label fx:id="bottomLabel" maxWidth="1.7976931348623157E308" styleClass="bold-blue-label" stylesheets="@../styles/MyBox.css" BorderPane.alignment="CENTER_LEFT">
         <BorderPane.margin>
            <Insets left="10.0" right="10.0" />
         </BorderPane.margin></Label>
    </bottom>
</BorderPane>
