<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="750.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImageManufactureEffectsController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <children>
                <HBox fx:id="hotBox" alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                    <children>
                        <ToolBar HBox.hgrow="ALWAYS">
                            <items>
                                                              <Label fx:id="tipsLabel" text="%Tips" />
                                <Hyperlink onAction="#showHelp" text="%Help" />

                                <Button mnemonicParsing="false" visible="false" />
                            </items>
                        </ToolBar>
                        <ToolBar fx:id="hotBar" disable="true" maxWidth="1.7976931348623157E308" HBox.hgrow="NEVER">
                            <items>
                                <CheckBox fx:id="showRefCheck" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%ShowReference" />
                                <CheckBox fx:id="showScopeCheck" mnemonicParsing="false" text="%ShowScope" />
                                <Button fx:id="outButton" mnemonicParsing="false" onAction="#zoomOut" text="%ZoomOut" />
                                <Button fx:id="inButton" mnemonicParsing="false" onAction="#zoomIn" text="%ZoomIn" />
                                <Button fx:id="wButton" mnemonicParsing="false" onAction="#paneSize" text="%PaneSize" />
                                <Button fx:id="oButton" mnemonicParsing="false" onAction="#imageSize" text="%ImageSize" />
                                <ComboBox fx:id="hisBox" prefWidth="120.0" promptText="%Histories" />
                                <Button fx:id="undoButton" disable="true" mnemonicParsing="false" onAction="#undoAction" text="%Undo" />
                                <Button fx:id="redoButton" disable="true" mnemonicParsing="false" onAction="#redoAction" text="%Redo" />
                                <Button fx:id="recoverButton" disable="true" mnemonicParsing="false" onAction="#recovery" text="%Recover" />
                                <Button fx:id="saveButton" disable="true" mnemonicParsing="false" onAction="#save" text="%Save" />
                            </items>
                        </ToolBar>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" minHeight="165.0" VBox.vgrow="NEVER">
                    <children>
                        <TabPane fx:id="tabPane" maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" tabClosingPolicy="UNAVAILABLE" HBox.hgrow="ALWAYS">
                            <tabs>
                                <Tab fx:id="fileTab" text="%File" />
                                <Tab fx:id="sizeTab" text="%Size" />
                                <Tab fx:id="cropTab" text="%Crop" />
                                <Tab fx:id="colorTab" text="%Color" />
                                <Tab fx:id="effectsTab" text="%Effects">
                                    <content>
                                        <VBox alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT">
                                            <children>
                                                <HBox alignment="CENTER_RIGHT">
                                                    <children>
                                                        <ToolBar HBox.hgrow="ALWAYS">
                                                            <items>
                                                                <RadioButton mnemonicParsing="false" text=" " visible="false" />
                                                            </items>
                                                        </ToolBar>
                                                        <ToolBar maxWidth="1.7976931348623157E308" HBox.hgrow="NEVER">
                                                            <items>
                                                                <HBox alignment="CENTER_LEFT" spacing="5.0">
                                                                    <children>
                                                                        <Label text="%Scope" />
                                                                        <RadioButton mnemonicParsing="false" selected="true" text="%All">
                                                                            <toggleGroup>
                                                                                <ToggleGroup fx:id="scopeGroup" />
                                                                            </toggleGroup>
                                                                        </RadioButton>
                                                                        <RadioButton mnemonicParsing="false" text="%Rectangle" toggleGroup="$scopeGroup" />
                                                                        <RadioButton mnemonicParsing="false" text="%Circle" toggleGroup="$scopeGroup" />
                                                                        <RadioButton mnemonicParsing="false" text="%Matting" toggleGroup="$scopeGroup" />
                                                                        <RadioButton mnemonicParsing="false" text="%ColorMatching" toggleGroup="$scopeGroup" />
                                                                        <RadioButton mnemonicParsing="false" text="%RectangleColor" toggleGroup="$scopeGroup" />
                                                                        <RadioButton mnemonicParsing="false" text="%CircleColor" toggleGroup="$scopeGroup" />
                                                                    </children>
                                                                </HBox>
                                                            </items>
                                                        </ToolBar>
                                                    </children>
                                                </HBox>
                                                <HBox alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT" VBox.vgrow="NEVER">
                                                    <children>
                                                        <ToolBar HBox.hgrow="ALWAYS">
                                                            <items>
                                                                <RadioButton mnemonicParsing="false" prefWidth="2.0" text=" " visible="false" />
                                                            </items>
                                                        </ToolBar>
                                                        <ToolBar maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="NEVER">
                                                            <items>
                                                                <Label text="%Effect" />
                                                                <RadioButton mnemonicParsing="false" selected="true" text="%Blur">
                                                                    <toggleGroup>
                                                                        <ToggleGroup fx:id="effectsGroup" />
                                                                    </toggleGroup>
                                                                </RadioButton>
                                                                <RadioButton mnemonicParsing="false" text="%Clarity" toggleGroup="$effectsGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Sharpen" toggleGroup="$effectsGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Emboss" toggleGroup="$effectsGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%EdgeDetection" toggleGroup="$effectsGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Posterizing" toggleGroup="$effectsGroup" />
                                                                <RadioButton fx:id="thresholdingRadio" mnemonicParsing="false" text="%Thresholding" toggleGroup="$effectsGroup" />
                                                                <RadioButton fx:id="grayRadio" mnemonicParsing="false" text="%Gray" toggleGroup="$effectsGroup" />
                                                                <RadioButton fx:id="bwRadio" mnemonicParsing="false" text="%BlackOrWhite" toggleGroup="$effectsGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Sepia" toggleGroup="$effectsGroup" />
                                                            </items>
                                                        </ToolBar>
                                                    </children>
                                                </HBox>
                                                <HBox alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT">
                                                    <children>
                                                        <ToolBar HBox.hgrow="ALWAYS">
                                                            <items>
                                                                <Label fx:id="promptLabel" styleClass="bold-blue-label" stylesheets="@../styles/MyBox.css" />
                                                                <Button mnemonicParsing="false" prefWidth="2.0" text=" " visible="false" />
                                                            </items>
                                                        </ToolBar>
                                                        <ToolBar HBox.hgrow="NEVER">
                                                            <items>
                                                                <HBox alignment="CENTER_LEFT" spacing="5.0">
                                                                    <children>
                                                                        <Label fx:id="intLabel" text="%Radius" />
                                                                        <ComboBox fx:id="intBox" editable="true" prefWidth="80.0" />
                                                                        <Label fx:id="stringLabel" text="Label" />
                                                                        <ComboBox fx:id="stringBox" prefWidth="100.0" />
                                                                    </children>
                                                                </HBox>
                                                                <CheckBox fx:id="grayCheck" mnemonicParsing="false" selected="true" text="%Gray" />
                                                                <HBox fx:id="thresholdingBox1" alignment="CENTER_LEFT" spacing="5.0">
                                                                    <children>
                                                                        <Label fx:id="thresholdLabel" text="%Threshold" />
                                                                        <TextField fx:id="thresholdingInput" prefWidth="50.0" promptText="0~255" />
                                                                    </children>
                                                                </HBox>
                                                                <HBox fx:id="thresholdingBox2" alignment="CENTER_LEFT" spacing="5.0">
                                                                    <children>
                                                                        <Label fx:id="smallLabel" text="%SmallValue" />
                                                                        <TextField fx:id="thresholdingMinInput" prefWidth="50.0" promptText="0~255" />
                                                                        <Label fx:id="bigLabel" text="%BigValue" />
                                                                        <TextField fx:id="thresholdingMaxInput" prefWidth="50.0" promptText="0~255" />
                                                                    </children>
                                                                </HBox>
                                                                <Button fx:id="calculateButton" mnemonicParsing="false" onAction="#calculateAction" text="%Calculate" />
                                                                <Button fx:id="okButton" mnemonicParsing="false" onAction="#effectsAction" text="%OK" />
                                                            </items>
                                                        </ToolBar>
                                                    </children>
                                                </HBox>
                                            </children>
                                        </VBox>
                                    </content>
                                </Tab>
                                <Tab fx:id="convolutionTab" text="%Convolution" />
                                <Tab fx:id="textTab" text="%Text" />
                                <Tab fx:id="coverTab" text="%Cover" />
                                <Tab fx:id="arcTab" text="%Arc" />
                                <Tab fx:id="shadowTab" text="%Shadow" />
                                <Tab fx:id="transformTab" text="%Transform" />
                                <Tab fx:id="marginsTab" text="%Margins" />
                                <Tab fx:id="viewTab" text="%View" />
                                <Tab fx:id="refTab" text="%Reference" />
                                <Tab fx:id="browseTab" text="%Browse" />
                            </tabs>
                        </TabPane>
                    </children>
                </HBox>
                <SplitPane fx:id="splitPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                    <items>
                        <VBox fx:id="imageBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <children>
                                <HBox fx:id="imageLabelBox" alignment="CENTER" spacing="5.0" VBox.vgrow="NEVER">
                                    <children>
                                        <Label fx:id="imageLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" text="%CurrentImage" wrapText="true" />
                                        <Button mnemonicParsing="false" visible="false" />
                                    </children>
                                </HBox>
                                <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true" VBox.vgrow="ALWAYS">
                                    <content>
                                        <ImageView fx:id="imageView" cache="true" fitHeight="546.0" fitWidth="1067.0" onMouseClicked="#clickImage" onMouseEntered="#setBottomLabel" pickOnBounds="true" preserveRatio="true">
                                            <cursor>
                                                <Cursor fx:constant="OPEN_HAND" />
                                            </cursor>
                                        </ImageView>
                                    </content>
                                </ScrollPane>
                            </children>
                        </VBox>
                    </items>
                </SplitPane>
            </children>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
        </VBox>
    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <bottom>
        <Label fx:id="bottomLabel" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER_LEFT" />
    </bottom>
</BorderPane>
