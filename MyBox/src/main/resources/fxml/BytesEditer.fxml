<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="750.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.BytesEditerController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" spacing="5.0" VBox.vgrow="NEVER">
                    <children>
                          <Button fx:id="selectSourceButton" mnemonicParsing="false" onAction="#selectSourceFile" onMouseEntered="#popSourceFile">
                              <graphic>
                                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                      <image>
                                          <Image url="@../buttons/iconOpen.png" />
                                      </image>
                                  </ImageView>
                              </graphic>
                          </Button>
                          <Button fx:id="createButton" mnemonicParsing="false" onAction="#createAction">
                              <graphic>
                                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                      <image>
                                          <Image url="@../buttons/iconAddFile.png" />
                                      </image>
                                  </ImageView>
                              </graphic>
                          </Button>
                          <ImageView fx:id="tipsView" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                  <Image url="@../buttons/iconTips.png" />
                              </image>
                          </ImageView>
                          <Label fx:id="editLabel" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                              <CheckBox fx:id="displayCheck" mnemonicParsing="false" text="%DecodeByCharset" />
                              <ComboBox fx:id="currentBox" prefWidth="150.0" />

                          <Button fx:id="saveAsButton" mnemonicParsing="false" onAction="#saveAsAction" onMouseEntered="#popSaveAs">
                              <graphic>
                                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                      <image>
                                          <Image url="@../buttons/iconSaveAs.png" />
                                      </image>
                                  </ImageView>
                              </graphic>
                          </Button>
                          <Button fx:id="recoverButton" mnemonicParsing="false" onAction="#recoverAction">
                              <graphic>
                                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                      <image>
                                          <Image url="@../buttons/iconRecover.png" />
                                      </image>
                                  </ImageView>
                              </graphic>
                          </Button>
                          <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveAction" styleClass="main-button" stylesheets="@../styles/MyBox.css" text="%Save" />
                    </children>
                </HBox>
                <TabPane fx:id="tabPane" nodeOrientation="RIGHT_TO_LEFT" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="NEVER">
                    <tabs>
                        <Tab fx:id="fileTab" text="%File">
                            <content>
                                <HBox nodeOrientation="LEFT_TO_RIGHT">
                                    <children>
                                        <ToolBar prefHeight="40.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                            <items>
                                                <RadioButton fx:id="loadRadio" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" selected="true" text="%LoadAfterSaveAs">
                                                    <toggleGroup>
                                                        <ToggleGroup fx:id="saveAsGroup" />
                                                    </toggleGroup>
                                                </RadioButton>
                                                <RadioButton fx:id="openRadio" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%OpenAfterSaveAs" toggleGroup="$saveAsGroup" />
                                                <RadioButton fx:id="justRadio" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%JustSaveAs" toggleGroup="$saveAsGroup" />
                                                <Button mnemonicParsing="false" text=" " visible="false" />
                                            </items>
                                        </ToolBar>
                                        <ToolBar HBox.hgrow="NEVER">
                                            <items>
                                                <Button fx:id="infoButton" mnemonicParsing="false" onAction="#infoAction" visible="false">
                                                    <graphic>
                                                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                                            <image>
                                                                <Image url="@../buttons/iconInfo.png" />
                                                            </image>
                                                        </ImageView>
                                                    </graphic>
                                                </Button>
                                                <CheckBox fx:id="confirmCheck" mnemonicParsing="false" selected="true" text="%ConfirmWhenSave" />
                                            </items>
                                        </ToolBar>
                                    </children>
                                </HBox>
                            </content>
                        </Tab>
                        <Tab fx:id="inputTab" text="%Input">
                            <content>
                                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                                    <children>
                                        <ToolBar HBox.hgrow="ALWAYS">
                                            <items>
                                                <Hyperlink fx:id="asciiLink" onAction="#openAscii" text="%AsciiTable" />
                                                <Button mnemonicParsing="false" text=" " visible="false" />
                                            </items>
                                        </ToolBar>
                                        <ToolBar HBox.hgrow="NEVER">
                                            <items>
                                                <Label text="%Symbol" />
                                                <ComboBox fx:id="symbolBox" prefWidth="120.0" />
                                                <Label text="%CapitalLetter" />
                                                <ComboBox fx:id="capitalBox" prefWidth="80.0" />
                                                <Label text="%SmallLetter" />
                                                <ComboBox fx:id="smallBox" prefWidth="80.0" />
                                                <Label text="%Number" />
                                                <ComboBox fx:id="numberBox" prefWidth="80.0" />
                                            </items>
                                        </ToolBar>
                                    </children>
                                </HBox>
                            </content>
                        </Tab>
                        <Tab fx:id="lbTab" text="%BreakLine">
                            <content>
                                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                                    <children>
                                        <ToolBar HBox.hgrow="ALWAYS">
                                            <items>
                                                <Button mnemonicParsing="false" text=" " visible="false" />
                                            </items>
                                        </ToolBar>
                                        <ToolBar HBox.hgrow="NEVER">
                                            <items>
                                                <RadioButton fx:id="bytesNumberRadio" mnemonicParsing="false" selected="true" text="%BytesNumber">
                                                    <toggleGroup>
                                                        <ToggleGroup fx:id="lineBreakGroup" />
                                                    </toggleGroup>
                                                </RadioButton>
                                                <TextField fx:id="bytesNumberInput" prefWidth="60.0" text="30" />
                                                <RadioButton mnemonicParsing="false" text="%LFHex" toggleGroup="$lineBreakGroup" />
                                                <RadioButton mnemonicParsing="false" text="%CRHex" toggleGroup="$lineBreakGroup" />
                                                <RadioButton fx:id="byteRadio" mnemonicParsing="false" text="%BytesHex" toggleGroup="$lineBreakGroup" />
                                                <TextField fx:id="hexInput" prefWidth="150.0" text="0D 0A" />
                                            </items>
                                        </ToolBar>
                                    </children>
                                </HBox>
                            </content>
                        </Tab>
                        <Tab fx:id="replaceTab" text="%FindReplace">
                            <content>
                                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                                    <children>
                                        <ToolBar HBox.hgrow="ALWAYS">
                                            <items>
                                                <Button mnemonicParsing="false" text=" " visible="false" />
                                            </items>
                                        </ToolBar>
                                        <ToolBar HBox.hgrow="NEVER">
                                            <items>
                                                <RadioButton mnemonicParsing="false" selected="true" text="%CurrentPage">
                                                    <toggleGroup>
                                                        <ToggleGroup fx:id="findGroup" />
                                                    </toggleGroup>
                                                </RadioButton>
                                                <RadioButton mnemonicParsing="false" text="%WholeFile" toggleGroup="$findGroup" />
                                                <TextField fx:id="findInput" prefWidth="150.0" />
                                                <HBox fx:id="findBox" alignment="CENTER_LEFT" spacing="5.0">
                                                    <children>
                                                        <Button fx:id="findFirstButton" disable="true" mnemonicParsing="false" onAction="#findFirstAction">
                                                            <graphic>
                                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                                    <image>
                                                                        <Image url="@../buttons/iconFirst.png" />
                                                                    </image>
                                                                </ImageView>
                                                            </graphic>
                                                        </Button>
                                                        <Button fx:id="findNextButton" disable="true" mnemonicParsing="false" onAction="#findNextAction">
                                                            <graphic>
                                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                                    <image>
                                                                        <Image url="@../buttons/iconNext.png" />
                                                                    </image>
                                                                </ImageView>
                                                            </graphic>
                                                        </Button>
                                                        <Button fx:id="findPreviousButton" disable="true" mnemonicParsing="false" onAction="#findPreviousAction">
                                                            <graphic>
                                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                                    <image>
                                                                        <Image url="@../buttons/iconPrevious.png" />
                                                                    </image>
                                                                </ImageView>
                                                            </graphic>
                                                        </Button>
                                                        <Button fx:id="findLastButton" disable="true" mnemonicParsing="false" onAction="#findLastAction">
                                                            <graphic>
                                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                                    <image>
                                                                        <Image url="@../buttons/iconLast.png" />
                                                                    </image>
                                                                </ImageView>
                                                            </graphic>
                                                        </Button>
                                                        <Button fx:id="countButton" disable="true" mnemonicParsing="false" onAction="#countAction" text="%Count" />
                                                        <TextField fx:id="replaceInput" prefWidth="150.0" />
                                                        <Button fx:id="replaceButton" disable="true" mnemonicParsing="false" onAction="#replaceAction">
                                                            <graphic>
                                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                                    <image>
                                                                        <Image url="@../buttons/iconReplace.png" />
                                                                    </image>
                                                                </ImageView>
                                                            </graphic>
                                                        </Button>
                                                        <Button fx:id="replaceAllButton" disable="true" mnemonicParsing="false" onAction="#replaceAllAction">
                                                            <graphic>
                                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                                    <image>
                                                                        <Image url="@../buttons/iconReplaceAll.png" />
                                                                    </image>
                                                                </ImageView>
                                                            </graphic>
                                                        </Button>
                                                    </children>
                                                </HBox>
                                            </items>
                                        </ToolBar>
                                    </children>
                                </HBox>
                            </content>
                        </Tab>
                        <Tab fx:id="filterTab" text="%FilterLines">
                            <content>
                                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                                    <children>
                                        <ToolBar HBox.hgrow="ALWAYS">
                                            <items>
                                                <Button mnemonicParsing="false" text=" " visible="false" />
                                            </items>
                                        </ToolBar>
                                        <ToolBar HBox.hgrow="NEVER">
                                            <items>
                                                <HBox fx:id="filterTypesBox" alignment="CENTER_LEFT" spacing="5.0">
                                                    <children>
                                                        <RadioButton fx:id="filterIncludeRadio" mnemonicParsing="false" selected="true" text="%IncludeOne">
                                                            <toggleGroup>
                                                                <ToggleGroup fx:id="filterGroup" />
                                                            </toggleGroup>
                                                        </RadioButton>
                                                        <RadioButton mnemonicParsing="false" text="%IncludeAll" toggleGroup="$filterGroup" />
                                                        <RadioButton mnemonicParsing="false" text="%NotIncludeAll" toggleGroup="$filterGroup" />
                                                        <RadioButton mnemonicParsing="false" text="%NotIncludeAny" toggleGroup="$filterGroup" />
                                                    </children>
                                                </HBox>
                                                <HBox fx:id="filterBox" alignment="CENTER_LEFT" spacing="5.0">
                                                    <children>
                                                        <TextField fx:id="filterInput" prefWidth="300.0" promptText="%SeparateByCommaBlanksInvolved" />
                                                        <CheckBox fx:id="filterLineNumberCheck" mnemonicParsing="false" selected="true" text="%LineNumber" />
                                                        <Button fx:id="filterButton" disable="true" mnemonicParsing="false" onAction="#filterAction">
                                                            <graphic>
                                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                                    <image>
                                                                        <Image url="@../buttons/iconFilter.png" />
                                                                    </image>
                                                                </ImageView>
                                                            </graphic>
                                                        </Button>
                                                    </children>
                                                </HBox>
                                            </items>
                                        </ToolBar>
                                    </children>
                                </HBox>
                            </content>
                        </Tab>
                        <Tab fx:id="locateTab" text="%Locate">
                            <content>
                                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                                    <children>
                                        <ToolBar HBox.hgrow="ALWAYS">
                                            <items>
                                                <Button mnemonicParsing="false" text=" " visible="false" />
                                            </items>
                                        </ToolBar>
                                        <ToolBar HBox.hgrow="NEVER">
                                            <items>
                                                <Label text="%Line" />
                                                <TextField fx:id="lineInput" prefWidth="120.0" />
                                                <Button fx:id="locateLineButton" mnemonicParsing="false" onAction="#locateLine">
                                                    <graphic>
                                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                            <image>
                                                                <Image url="@../buttons/iconGo.png" />
                                                            </image>
                                                        </ImageView>
                                                    </graphic>
                                                </Button>
                                                <Label text="%Byte">
                                                    <padding>
                                                        <Insets left="50.0" />
                                                    </padding>
                                                </Label>
                                                <TextField fx:id="objectNumberInput" prefWidth="120.0" />
                                                <Button fx:id="locateObjectButton" mnemonicParsing="false" onAction="#locateObject">
                                                    <graphic>
                                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                            <image>
                                                                <Image url="@../buttons/iconGo.png" />
                                                            </image>
                                                        </ImageView>
                                                    </graphic>
                                                </Button>
                                            </items>
                                        </ToolBar>
                                    </children>
                                </HBox>
                            </content>
                        </Tab>
                        <Tab fx:id="textTab" text="%Text">
                            <content>
                                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                                    <children>
                                        <ToolBar HBox.hgrow="ALWAYS">
                                            <items>
                                                <Button mnemonicParsing="false" text=" " visible="false" />
                                            </items>
                                        </ToolBar>
                                        <ToolBar HBox.hgrow="NEVER">
                                            <items>
                                                <Label fx:id="bomLabel" />
                                                <CheckBox fx:id="scrollCheck" mnemonicParsing="false" selected="true" text="%ScrollSynchronously" />
                                            </items>
                                        </ToolBar>
                                    </children>
                                </HBox>
                            </content>
                        </Tab>
                        <Tab fx:id="pageTab" text="%Paginate">
                            <content>
                                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                                    <children>
                                        <ToolBar HBox.hgrow="ALWAYS">
                                            <items>
                                                <Button mnemonicParsing="false" text=" " visible="false" />
                                            </items>
                                        </ToolBar>
                                        <ToolBar HBox.hgrow="NEVER">
                                            <items>
                                                <Label text="%PageSize" />
                                                <Label text="%BytesPaginateComment" />
                                                <TextField fx:id="pageSizeInput" prefWidth="80.0" text="100000" />
                                                <Button fx:id="okButton" mnemonicParsing="false" onAction="#okAction">
                                                    <graphic>
                                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                            <image>
                                                                <Image url="@../buttons/iconOK.png" />
                                                            </image>
                                                        </ImageView>
                                                    </graphic>
                                                </Button>
                                            </items>
                                        </ToolBar>
                                    </children>
                                </HBox>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
                <SplitPane fx:id="splitPane" prefHeight="160.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                    <items>
                        <AnchorPane fx:id="textPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <children>
                                <TextArea fx:id="lineArea" maxHeight="1.7976931348623157E308" prefWidth="150.0" styleClass="grayArea" stylesheets="@../styles/MyBox.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                <TextArea fx:id="mainArea" layoutX="24.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" promptText="%EditHere" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="75.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                            </children>
                        </AnchorPane>
                    </items>
                </SplitPane>
                <HBox alignment="CENTER_LEFT" VBox.vgrow="NEVER">
                    <children>
                        <ToolBar fx:id="rangeBar" HBox.hgrow="ALWAYS">
                            <items>
                                <Button mnemonicParsing="false" text=" " visible="false" />
                            </items>
                        </ToolBar>
                        <ToolBar fx:id="pageBar" disable="true" HBox.hgrow="NEVER">
                            <items>
                                <Button fx:id="firstButton" mnemonicParsing="false" onAction="#firstAction">
                                    <graphic>
                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../buttons/iconFirst.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="previousButton" mnemonicParsing="false" onAction="#previousAction">
                                    <graphic>
                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../buttons/iconPrevious.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextAction">
                                    <graphic>
                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../buttons/iconNext.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="lastButton" mnemonicParsing="false" onAction="#lastAction">
                                    <graphic>
                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../buttons/iconLast.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Label text="%Page" />
                                <TextField fx:id="pageInput" prefWidth="60.0" text="1" />
                                <Label fx:id="pageLabel" />
                                <Button fx:id="goButton" mnemonicParsing="false" onAction="#goPageAction">
                                    <graphic>
                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../buttons/iconGo.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                            </items>
                        </ToolBar>
                    </children>
                </HBox>
            </children>
        </VBox>
    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <bottom>
        <HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="bottomLabel" HBox.hgrow="ALWAYS" />
                <Label fx:id="selectionLabel" HBox.hgrow="NEVER">
                    <HBox.margin>
                        <Insets left="20.0" />
                    </HBox.margin>
                </Label>
            </children>
        </HBox>
    </bottom>
</BorderPane>
