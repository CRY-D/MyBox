<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="730.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImagesBrowserController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox fx:id="mainPane" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
                    <children>
                        <ToolBar HBox.hgrow="ALWAYS">
                            <items>
                                <Button fx:id="selectButton" mnemonicParsing="false" onAction="#selectImages" onMouseEntered="#popSourcePath" text="%Select..." />
                                <Label fx:id="tipsLabel" styleClass="darkRed-label" stylesheets="@../styles/MyBox.css" text="%Tips" />
                            </items>
                        </ToolBar>
                        <ToolBar fx:id="setBar" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="NEVER">
                            <items>
                                <Label text="%Files" />
                                <ComboBox fx:id="filesBox" editable="true" prefWidth="80.0" />
                                <Label fx:id="totalLabel">
                                    <padding>
                                        <Insets right="20.0" />
                                    </padding>
                                </Label>
                                <Label text="%Columns" />
                                <ComboBox fx:id="colsnumBox" editable="true" prefWidth="120.0" />
                        <Button mnemonicParsing="false" onAction="#thumbsListAction" text="%ThumbnailsList" />
                        <Button mnemonicParsing="false" onAction="#filesListAction" text="%FilesList" />
                                <Label text="%LoadWidth" />
                                <ComboBox fx:id="loadWidthBox" editable="true" prefWidth="140.0" />
                            </items>
                        </ToolBar>
                    </children>
                </HBox>
                <HBox fx:id="opBox" alignment="CENTER" VBox.vgrow="NEVER">
                    <children>
                        <ToolBar HBox.hgrow="ALWAYS">
                            <items>
                                <Button mnemonicParsing="false" prefWidth="1.0" visible="false" />
                            </items>
                        </ToolBar>
                        <ToolBar fx:id="opBar" maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" HBox.hgrow="NEVER">
                            <items>
                                <HBox fx:id="opPane" alignment="CENTER" spacing="5.0">
                                    <children>
                                        <HBox fx:id="opBox3" alignment="CENTER" spacing="5.0">
                                            <children>
                                                <Button fx:id="imageSizeButton" mnemonicParsing="false" onAction="#loadedSize" text="%LoadedSize" />
                                                <Button fx:id="paneSizeButton" defaultButton="true" mnemonicParsing="false" onAction="#paneSize" text="%PaneSize" />
                                                <Button fx:id="zoomInButton" mnemonicParsing="false" onAction="#zoomIn" text="%ZoomIn" />
                                                <Button fx:id="zoomOutButton" mnemonicParsing="false" onAction="#zoomOut" text="%ZoomOut" />
                                                <Button fx:id="mlButton" mnemonicParsing="false" onAction="#moveLeft" text="%Right" />
                                                <Button fx:id="upButton" mnemonicParsing="false" onAction="#moveUp" text="%Up" />
                                                <Button fx:id="downButton" mnemonicParsing="false" onAction="#moveDown" text="%Down" />
                                                <Button fx:id="mrButton" mnemonicParsing="false" onAction="#moveRight" text="%Left" />
                                            </children>
                                        </HBox>
                              <HBox fx:id="rotateBox" alignment="CENTER_LEFT" spacing="5.0">
                                 <children>
                                              <Button fx:id="rButton" mnemonicParsing="false" onAction="#rotateRight" text="%RotateRight" />
                                              <Button fx:id="lButton" mnemonicParsing="false" onAction="#rotateLeft" text="%RotateLeft" />
                                              <Button fx:id="tButton" mnemonicParsing="false" onAction="#turnOver" text="%TurnOver" />
                                              <CheckBox fx:id="saveRotationCheck" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%SaveRotation" />
                                 </children>
                              </HBox>
                                    </children>
                                </HBox>
                            </items>
                        </ToolBar>
                    </children>
                </HBox>
                <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true" VBox.vgrow="ALWAYS">
                    <content>
                        <VBox fx:id="imagesPane" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" />
                    </content>
                </ScrollPane>
                <HBox fx:id="navBox">
                    <children>
                        <ToolBar fx:id="fileOpBar" prefWidth="200.0" HBox.hgrow="ALWAYS">
                            <items>
                                <Button fx:id="viewButton" mnemonicParsing="false" onAction="#viewAction" text="%View" />
                                <Button fx:id="infoButton" mnemonicParsing="false" onAction="#infoAction" text="%ImageInformation" />
                                <Button fx:id="renameButton" mnemonicParsing="false" onAction="#renameAction" text="%Rename" />
                                <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteAction" text="%Delete" />
                                <CheckBox fx:id="deleteConfirmCheck" mnemonicParsing="false" selected="true" text="%ConfirmWhenDelete" />
                            </items>
                        </ToolBar>
                        <ToolBar fx:id="navBar" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="NEVER">
                            <items>
                                <Label text="%Sort">
                                    <padding>
                                        <Insets left="20.0" />
                                    </padding>
                                </Label>
                        <ComboBox fx:id="sortBox" prefWidth="200.0" />
                                <Button fx:id="previousButton" mnemonicParsing="false" onAction="#previousAction" text="%PreviousBatch" />
                                <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextAction" text="%NextBacth" />
                            </items>
                        </ToolBar>
                    </children>
                </HBox>
            </children>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <bottom>
        <Label fx:id="bottomLabel" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER_LEFT" />
    </bottom>
</BorderPane>
