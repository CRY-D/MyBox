<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="thisPane" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.LoadingController">
    <children>
        <ProgressIndicator fx:id="progressIndicator">
            <VBox.margin>
                <Insets bottom="20.0" top="10.0" />
            </VBox.margin>
        </ProgressIndicator>
        <Label fx:id="timeLabel" />
        <Label fx:id="infoLabel" text="..." />
        <HBox alignment="CENTER" spacing="20.0">
            <children>
                <Button mnemonicParsing="false" onAction="#mybox" text="MyBox" />
                <Button mnemonicParsing="false" onAction="#cancelAction" text="%Cancel" />
            </children>
            <VBox.margin>
                <Insets top="10.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
