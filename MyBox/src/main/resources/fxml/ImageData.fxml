<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImageDataController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox fx:id="contentBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" BorderPane.alignment="CENTER">
            <children>
                <HBox fx:id="sourceBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
                    <children>
                        <Button fx:id="selectSourceButton" mnemonicParsing="false" onAction="#selectSourceFile" onMouseEntered="#popSourceFile">
                            <graphic>
                                <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../buttons/iconOpen.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Label fx:id="sampledTips" maxWidth="1.7976931348623157E308" styleClass="darkRed-label" stylesheets="@../styles/MyBox.css" text="%Sampled" visible="false" HBox.hgrow="ALWAYS" />
                        <Label alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" styleClass="blue-label" stylesheets="@../styles/MyBox.css" HBox.hgrow="ALWAYS" fx:id="xyLabel" />
                        <Label fx:id="imageLabel" alignment="CENTER" wrapText="true" HBox.hgrow="NEVER" />
                        <HBox fx:id="opBox" alignment="CENTER_LEFT" spacing="5.0" HBox.hgrow="NEVER">
                            <children>
                                <CheckBox fx:id="selectAreaCheck" mnemonicParsing="false">
                                    <graphic>
                                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../buttons/iconTarget.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </CheckBox>
                                <HBox fx:id="manageBox" alignment="CENTER_LEFT" spacing="5.0">
                                    <children>
                                        <CheckBox fx:id="topCheck" mnemonicParsing="false">
                                            <graphic>
                                                <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../buttons/iconTop.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </CheckBox>
                                        <CheckBox fx:id="coordinateCheck" mnemonicParsing="false">
                                            <graphic>
                                                <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../buttons/iconLocation.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </CheckBox>
                                        <CheckBox fx:id="rulerXCheck" mnemonicParsing="false">
                                            <graphic>
                                                <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../buttons/iconXRuler.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </CheckBox>
                                        <CheckBox fx:id="rulerYCheck" mnemonicParsing="false">
                                            <graphic>
                                                <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../buttons/iconYRuler.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </CheckBox>
                                        <Button fx:id="zoomOutButton" mnemonicParsing="false" onAction="#zoomOut">
                                            <graphic>
                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../buttons/iconZoomOut.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                        <Button fx:id="zoomInButton" mnemonicParsing="false" onAction="#zoomIn">
                                            <graphic>
                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../buttons/iconZoomIn.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                        <Button fx:id="paneSizeButton" mnemonicParsing="false" onAction="#paneSize">
                                            <graphic>
                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../buttons/iconPicBig.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                        <Button fx:id="imageSizeButton" mnemonicParsing="false" onAction="#loadedSize">
                                            <graphic>
                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../buttons/iconPicSmall.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                    </children>
                                </HBox>
                                <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#refreshData">
                                    <graphic>
                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../buttons/iconRefresh.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </HBox>
                <SplitPane fx:id="dataPane" dividerPositions="0.5" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" VBox.vgrow="ALWAYS">
                    <items>
                        <SplitPane dividerPositions="0.6">
                            <items>
                                <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true" prefViewportHeight="374.0" prefViewportWidth="696.0">
                                    <content>
                                        <TableView fx:id="dataTable" style="-fx-font-size: 14;" tableMenuButtonVisible="true">
                                            <columns>
                                                <TableColumn fx:id="colorColumn" prefWidth="83.0" text="%Color" />
                                                <TableColumn fx:id="meanColumn" prefWidth="73.0" text="%Mean" />
                                                <TableColumn fx:id="varianceColumn" prefWidth="73.0" text="%Variance" />
                                                <TableColumn fx:id="skewnessColumn" prefWidth="82.0" text="%Skewness" />
                                                <TableColumn fx:id="medianColumn" prefWidth="71.0" text="%Median" />
                                                <TableColumn fx:id="modeColumn" prefWidth="76.0" text="%Mode" />
                                                <TableColumn fx:id="maximumColumn" prefWidth="82.0" text="%Maximum" />
                                                <TableColumn fx:id="minimumColumn" prefWidth="81.0" text="%Minimum" />
                                            </columns>
                                        </TableView>
                                    </content>
                                </ScrollPane>
                                <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" pannable="true" prefViewportHeight="504.0" prefViewportWidth="538.0">
                                    <content>
                                        <AnchorPane fx:id="maskPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onMouseClicked="#paneClicked" onMouseDragged="#showXY" onMouseMoved="#showXY" onMousePressed="#showXY" onMouseReleased="#showXY" VBox.vgrow="ALWAYS">
                                            <children>
                                                <ImageView fx:id="imageView" cache="true" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                                </ImageView>
                                                <Rectangle fx:id="maskRectangleLine" arcHeight="5.0" arcWidth="5.0" fill="#ffffff00" height="170.0" layoutX="143.0" layoutY="52.0" onMousePressed="#handlerPressed" onMouseReleased="#rectangleReleased" stroke="RED" strokeType="INSIDE" width="283.0">
                                                    <cursor>
                                                        <Cursor fx:constant="MOVE" />
                                                    </cursor>
                                                </Rectangle>
                                                <Rectangle fx:id="leftCenterHandler" fill="#ffffff00" height="10.0" layoutX="158.0" layoutY="193.0" onMousePressed="#handlerPressed" onMouseReleased="#leftCenterHandlerReleased" stroke="BLUE" strokeType="INSIDE" width="10.0">
                                                    <cursor>
                                                        <Cursor fx:constant="W_RESIZE" />
                                                    </cursor>
                                                </Rectangle>
                                                <Rectangle fx:id="topRightHandler" fill="#ffffff00" height="10.0" layoutX="492.0" layoutY="72.0" onMousePressed="#handlerPressed" onMouseReleased="#topRightHandlerReleased" stroke="BLUE" strokeType="INSIDE" width="10.0">
                                                    <cursor>
                                                        <Cursor fx:constant="NE_RESIZE" />
                                                    </cursor>
                                                </Rectangle>
                                                <Rectangle fx:id="topCenterHandler" fill="#ffffff00" height="10.0" layoutX="325.0" layoutY="72.0" onMousePressed="#handlerPressed" onMouseReleased="#topCenterHandlerReleased" stroke="BLUE" strokeType="INSIDE" width="10.0">
                                                    <cursor>
                                                        <Cursor fx:constant="N_RESIZE" />
                                                    </cursor>
                                                </Rectangle>
                                                <Rectangle fx:id="rightCenterHandler" fill="#ffffff00" height="10.0" layoutX="492.0" layoutY="193.0" onMousePressed="#handlerPressed" onMouseReleased="#rightCenterHandlerReleased" stroke="BLUE" strokeType="INSIDE" width="10.0">
                                                    <cursor>
                                                        <Cursor fx:constant="E_RESIZE" />
                                                    </cursor>
                                                </Rectangle>
                                                <Rectangle fx:id="bottomRightHandler" fill="#ffffff00" height="10.0" layoutX="492.0" layoutY="313.0" onMousePressed="#handlerPressed" onMouseReleased="#bottomRightHandlerReleased" stroke="BLUE" strokeType="INSIDE" width="10.0">
                                                    <cursor>
                                                        <Cursor fx:constant="SE_RESIZE" />
                                                    </cursor>
                                                </Rectangle>
                                                <Rectangle fx:id="bottomCenterHandler" fill="#ffffff00" height="10.0" layoutX="325.0" layoutY="313.0" onMousePressed="#handlerPressed" onMouseReleased="#bottomCenterHandlerReleased" stroke="BLUE" strokeType="INSIDE" width="10.0">
                                                    <cursor>
                                                        <Cursor fx:constant="N_RESIZE" />
                                                    </cursor>
                                                </Rectangle>
                                                <Rectangle fx:id="bottomLeftHandler" fill="#ffffff00" height="10.0" layoutX="158.0" layoutY="313.0" onMousePressed="#handlerPressed" onMouseReleased="#bottomLeftHandlerReleased" stroke="BLUE" strokeType="INSIDE" width="10.0">
                                                    <cursor>
                                                        <Cursor fx:constant="SW_RESIZE" />
                                                    </cursor>
                                                </Rectangle>
                                                <Rectangle fx:id="topLeftHandler" fill="#ffffff00" height="10.0" layoutX="158.0" layoutY="72.0" onMousePressed="#handlerPressed" onMouseReleased="#topLeftHandlerReleased" stroke="BLUE" strokeType="INSIDE" width="10.0">
                                                    <cursor>
                                                        <Cursor fx:constant="NW_RESIZE" />
                                                    </cursor>
                                                </Rectangle>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" x="200.0" y="200.0" fx:id="xyText" />
                                            </children>
                                        </AnchorPane>
                                    </content>
                                </ScrollPane>
                            </items>
                        </SplitPane>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <children>
                                <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" minHeight="-Infinity" spacing="2.0"  VBox.vgrow="NEVER">
                                    <children>
                                        <Label maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                                        <CheckBox fx:id="greyHistCheck" mnemonicParsing="false" text="%Grey" />
                                        <CheckBox fx:id="redHistCheck" mnemonicParsing="false" selected="true" text="%Red" />
                                        <CheckBox fx:id="greenHistCheck" mnemonicParsing="false" selected="true" text="%Green" />
                                        <CheckBox fx:id="blueHistCheck" mnemonicParsing="false" selected="true" text="%Blue" />
                                        <CheckBox fx:id="hueHistCheck" mnemonicParsing="false" text="%Hue" />
                                        <CheckBox fx:id="saturationHistCheck" mnemonicParsing="false" text="%Saturation" />
                                        <CheckBox fx:id="brightnessHistCheck" mnemonicParsing="false" text="%Brightness" />
                                        <CheckBox fx:id="alphaHistCheck" mnemonicParsing="false" text="%Alpha" />
                                        <Button mnemonicParsing="false" onAction="#clearHistogram" text="%Clear" />
                                    </children>
                                    <VBox.margin>
                                        <Insets top="5.0" />
                                    </VBox.margin>
                                </HBox>
                                <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true" prefViewportHeight="400.0" prefViewportWidth="1242.0" VBox.vgrow="ALWAYS">
                                    <content>
                                        <BarChart fx:id="histogramChart" alternativeRowFillVisible="false" barGap="1.0" categoryGap="1.0" horizontalGridLinesVisible="false" horizontalZeroLineVisible="false" legendSide="TOP" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" verticalGridLinesVisible="false" verticalZeroLineVisible="false">
                                            <xAxis>
                                                <CategoryAxis fx:id="grayX" animated="false" side="BOTTOM" tickLabelGap="1.0" tickLabelRotation="45.0" tickMarkVisible="false" />
                                            </xAxis>
                                            <yAxis>
                                                <NumberAxis fx:id="grayY" side="LEFT" tickLabelGap="1.0" tickLength="10.0" />
                                            </yAxis>
                                        </BarChart>
                                    </content>
                                </ScrollPane>
                            </children>
                        </VBox>
                    </items>
                </SplitPane>
            </children>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <bottom>
        <Label fx:id="bottomLabel" maxWidth="1.7976931348623157E308" styleClass="blue-label" stylesheets="@../styles/MyBox.css" BorderPane.alignment="CENTER" />
    </bottom>
</BorderPane>
