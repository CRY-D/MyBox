<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImagesBatchController">
    <children>
        <ToolBar maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
            <items>
                <Button fx:id="addFilesButton" mnemonicParsing="false" onAction="#addFilesAction" onMouseEntered="#popFileAdd">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconAddFile.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="insertFilesButton" mnemonicParsing="false" onAction="#insertFilesAction" onMouseEntered="#popFileInsert">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconInsertFile.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="moveUpButton" mnemonicParsing="false" onAction="#moveUpAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconUp.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="moveDownButton" mnemonicParsing="false" onAction="#moveDownAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconDown.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="deleteFilesButton" mnemonicParsing="false" onAction="#deleteFilesAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconDeleteFile.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clearFilesAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconClear.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="viewFileButton" mnemonicParsing="false" onAction="#viewFileAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconView.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="metaButton" mnemonicParsing="false" onAction="#metaAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconMeta.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="infoButton" mnemonicParsing="false" onAction="#infoAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconInfo.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <CheckBox fx:id="thumbCheck" mnemonicParsing="false" selected="true" text="%Thumbnail" />
            </items>
        </ToolBar>
        <TableView fx:id="tableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="900.0" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="handledColumn" prefWidth="174.0" text="%Handled" />
                <TableColumn fx:id="imageColumn" editable="false" prefWidth="150.0" text="%SourceImage" />
                <TableColumn fx:id="sizeColumn" prefWidth="150.0" text="%Pixels" />
                <TableColumn fx:id="typeColumn" prefWidth="100.0" text="%Color" />
                <TableColumn fx:id="fileColumn" prefWidth="600.0" text="%File" />
                <TableColumn fx:id="indexColumn" prefWidth="100.0" text="%Index" />
            </columns>
        </TableView>
        <Label fx:id="sourcesLabel" maxWidth="1.7976931348623157E308" styleClass="blue-label" stylesheets="@../styles/MyBox.css" text=" " VBox.vgrow="NEVER" />
    </children>
</VBox>
