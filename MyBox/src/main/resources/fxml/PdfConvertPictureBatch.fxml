<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="856.0" prefWidth="1096.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.PdfConvertPicturesBatchController">
    <top>
        <HBox id="MyBoxMenuBar" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../styles/MyBox.css" BorderPane.alignment="CENTER">
            <children>
                <HBox prefWidth="400.0">
                    <children>
                        <fx:include fx:id="titleBar" source="TitleBar.fxml" />
                    </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                    <children>
                        <fx:include fx:id="functionsBar" source="FunctionsBar.fxml" />
                        <fx:include fx:id="commonBar" source="CommonBar.fxml" />
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets left="5.0" right="5.0" />
            </padding>
            <BorderPane.margin>
                <Insets bottom="10.0" />
            </BorderPane.margin>
        </HBox>
    </top>
    <center>
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <children>
                <fx:include fx:id="pdfToolsBar" source="PdfToolsBar.fxml" />
                <VBox fx:id="paraBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <TitledPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%SourcePDF">
                            <content>
                                <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" prefWidth="1084.0">
                                    <children>
                                        <fx:include fx:id="filesTable" source="FilesTable.fxml" />
                                    </children>
                                </HBox>
                            </content>
                            <VBox.margin>
                                <Insets top="5.0" />
                            </VBox.margin>
                        </TitledPane>
                        <TitledPane maxWidth="1.7976931348623157E308" text="%TargetImages">
                            <content>
                                <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" prefWidth="1084.0">
                                    <children>
                                        <fx:include fx:id="imageAttributes" source="ImageAttributes.fxml" />
                                    </children>
                                </HBox>
                            </content>
                            <VBox.margin>
                                <Insets top="5.0" />
                            </VBox.margin>
                        </TitledPane>
                        <TitledPane maxWidth="1.7976931348623157E308" text="%TargetFiles">
                            <content>
                                <VBox prefHeight="44.0" prefWidth="1084.0">
                                    <children>
                                        <HBox spacing="10.0" VBox.vgrow="NEVER">
                                            <children>
                                                <Label text="%FilesPath">
                                                    <HBox.margin>
                                                        <Insets />
                                                    </HBox.margin>
                                                </Label>
                                                <TextField fx:id="targetPathInput" prefHeight="30.0" prefWidth="682.0" HBox.hgrow="ALWAYS" />
                                                <Button mnemonicParsing="false" onAction="#selectTargetPath" prefHeight="30.0" prefWidth="90.0" text="%Select..." />
                                                <Button fx:id="openTargetButton" disable="true" mnemonicParsing="false" onAction="#openTargetPath" prefHeight="30.0" prefWidth="80.0" text="%Open" />
                                            </children>
                                            <VBox.margin>
                                                <Insets left="10.0" right="10.0" />
                                            </VBox.margin>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" prefHeight="0.0" prefWidth="1040.0" spacing="10.0">
                                            <children>
                                                <Label text="%FileName">
                                                    <HBox.margin>
                                                        <Insets />
                                                    </HBox.margin>
                                                </Label>
                                                <CheckBox fx:id="fillZero" mnemonicParsing="false" selected="true" text="%FillZero">
                                                    <HBox.margin>
                                                        <Insets />
                                                    </HBox.margin>
                                                </CheckBox>
                                                <CheckBox fx:id="appendColor" mnemonicParsing="false" selected="true" text="%AppendImageColor" />
                                                <CheckBox fx:id="appendDensity" mnemonicParsing="false" selected="true" text="%AppendDensity">
                                                    <HBox.margin>
                                                        <Insets />
                                                    </HBox.margin>
                                                </CheckBox>
                                                <CheckBox fx:id="appendCompressionType" mnemonicParsing="false" selected="true" text="%AppendCompressionType" />
                                                <CheckBox fx:id="appendQuality" mnemonicParsing="false" selected="true" text="%AppendQuality" />
                                            </children>
                                            <VBox.margin>
                                                <Insets left="10.0" right="10.0" top="5.0" />
                                            </VBox.margin>
                                        </HBox>
                                    </children>
                                </VBox>
                            </content>
                            <VBox.margin>
                                <Insets top="5.0" />
                            </VBox.margin>
                        </TitledPane>
                    </children>
                </VBox>
                <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0">
                    <children>
                        <Button fx:id="startButton" disable="true" mnemonicParsing="false" onAction="#startConversion" prefHeight="30.0" prefWidth="80.0" text="%Start" />
                        <Button fx:id="pauseButton" disable="true" mnemonicParsing="false" onAction="#pauseConversion" prefHeight="30.0" prefWidth="100.0" text="%Pause" visible="false" />
                        <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" prefHeight="23.0" prefWidth="764.0" progress="0.0" HBox.hgrow="ALWAYS" />
                        <Label fx:id="progressValue" prefHeight="20.0" prefWidth="108.0" />
                    </children>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <TextField id="PdfStatus" fx:id="statusLabel" editable="false" focusTraversable="false" maxWidth="1.7976931348623157E308" prefHeight="14.0" prefWidth="1061.0" styleClass="copyable-label" stylesheets="@../styles/MyBox.css" HBox.hgrow="ALWAYS" />
                    </children>
                    <VBox.margin>
                        <Insets top="2.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</BorderPane>
