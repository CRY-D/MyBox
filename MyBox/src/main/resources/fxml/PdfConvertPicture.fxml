                                                            <Insets left="80.0" />
                                                         </HBox.margin>
                                                      </Label>
                                                      <CheckBox fx:id="appendDensity" mnemonicParsing="false" selected="true" text="%AppendDensity">
                                                         <HBox.margin>
                                                            <Insets />
                                                         </HBox.margin>
                                                      </CheckBox>
                                                      <CheckBox fx:id="appendType" mnemonicParsing="false" selected="true" text="%AppendImageType" />
