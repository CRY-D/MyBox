                                                                  <VBox.margin>
                                                                      <Insets left="10.0" right="10.0" top="10.0" />
                                                                  </VBox.margin>
                                                              </HBox>
                                                          </children>
                                                      </VBox>
                                                  </content>
                                                  <VBox.margin>
                                                      <Insets top="10.0" />
                                                  </VBox.margin>
                                              </TitledPane>
                                 </children>
                              </VBox>
                                        <HBox alignment="CENTER_LEFT" spacing="10.0">
                                            <children>
                                                <Button fx:id="startButton" disable="true" mnemonicParsing="false" onAction="#startConvertion" prefHeight="30.0" prefWidth="80.0" text="%Start" />
                                    <Button fx:id="pauseButton" disable="true" mnemonicParsing="false" onAction="#pauseConversion" prefHeight="30.0" prefWidth="86.0" text="%Pause" visible="false" />
                                                <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" prefHeight="23.0" prefWidth="764.0" progress="0.0" HBox.hgrow="ALWAYS" />
                                    <Label fx:id="progressValue" prefHeight="20.0" prefWidth="108.0" />
