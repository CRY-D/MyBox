                                                                  <children>
                                                                      <RadioButton mnemonicParsing="false" selected="true" text="png">
                                                         <toggleGroup>
                                                            <ToggleGroup fx:id="ImageFormatGroup" />
                                                         </toggleGroup>
                                                                      </RadioButton>
                                                                      <RadioButton mnemonicParsing="false" text="jpg" toggleGroup="$ImageFormatGroup" />
