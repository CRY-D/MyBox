<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.BarcodeCreatorController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox fx:id="contentBox" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" BorderPane.alignment="CENTER">
            <children>
            <HBox fx:id="codeBox" alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" spacing="5.0">
               <children>
                          <Label text="%Type" />
                          <ComboBox fx:id="typeSelecor" prefWidth="150.0" />
                          <Label text="%Code">
                     <padding>
                        <Insets left="40.0" />
                     </padding></Label>
                  <TextArea fx:id="codeInput" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="500.0" text="1234567890" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
                  <HBox fx:id="imageParaBox" alignment="CENTER_LEFT" spacing="5.0">
                      <children>
                  <Label text="%Orientation" />
                  <ComboBox fx:id="orientationSelecor" prefWidth="80.0" />
                  <Label text="%Resolution" />
                          <ComboBox fx:id="dpiSelector" prefWidth="80.0" />
                          <Label text="dpi" />
                      </children>
                  </HBox>
            <VBox fx:id="d1ParaBox" spacing="5.0" VBox.vgrow="NEVER">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="5.0">
                     <children>
                        <Label fx:id="label2" text="%Height" />
                        <TextField fx:id="height1Input" prefWidth="60.0" />
                        <Label fx:id="label1" text="%NarrowModuleWidth" />
                        <TextField fx:id="narrowWidthInput" prefWidth="60.0" />
                        <Label text="%BarRatio" />
                        <ComboBox fx:id="barRatioSelecor" editable="true" prefWidth="80.0" HBox.hgrow="NEVER" />
                        <Label fx:id="label11" text="%QuietZoneWidth" />
                        <TextField fx:id="quietWidthInput" prefWidth="60.0" />
                        <Button fx:id="suggestButton" mnemonicParsing="false" onAction="#suggestedSettings">
                           <graphic>
                              <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../buttons/iconIdea.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" spacing="5.0">
                     <children>
                        <Label text="%TextPosition" />
                        <ComboBox fx:id="textPositionSelector" prefWidth="100.0" />
                        <Label text="%FontFamily" />
                        <ComboBox fx:id="fontSelector" prefWidth="150.0" />
                        <Label text="%FontSize" />
                        <ComboBox fx:id="sizeSelector" editable="true" prefWidth="80.0" promptText="%Size" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox fx:id="qrParaBox" spacing="5.0" VBox.vgrow="NEVER">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
                     <children>
                        <Label text="%Width" />
                        <TextField fx:id="qrWidthInput" prefWidth="60.0" />
                        <Label text="%Height" />
                        <TextField fx:id="qrHeightInput" prefWidth="60.0" />
                        <Label text="%Margin" />
                        <TextField fx:id="qrMarginInput" prefWidth="60.0" />
                        <Label text="%ErrorCorrectionLevel" />
                        <ComboBox fx:id="qrErrorCorrectionSelecor" prefWidth="200.0" />
                        <Label text="%Picture" />
                        <TextField fx:id="sourceFileInput" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                        <Button fx:id="selectSourceButton" mnemonicParsing="false" onAction="#selectSourceFile" onMouseEntered="#popSourceFile">
                           <graphic>
                              <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../buttons/iconOpen.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox fx:id="pdf417ParaBox" layoutX="20.0" layoutY="247.0" spacing="5.0">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
                     <children>
                        <Label text="%Width" />
                        <TextField fx:id="pdf417WidthInput" prefWidth="60.0" />
                        <Label text="%Height" />
                        <TextField fx:id="pdf417HeightInput" prefWidth="60.0" />
                        <Label text="%Margin" />
                        <TextField fx:id="pdf417MarginInput" prefWidth="60.0" />
                        <Label text="%CompactionMode" />
                        <ComboBox fx:id="pdf417CompactionSelecor" prefWidth="100.0" />
                        <Label text="%ErrorCorrectionLevel" />
                        <ComboBox fx:id="pdf417ErrorCorrectionSelecor" prefWidth="200.0" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox fx:id="dmParaBox" layoutX="20.0" layoutY="357.0" spacing="5.0">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
                     <children>
                        <Label text="%Width" />
                        <TextField fx:id="dmWidthInput" prefWidth="60.0" />
                        <Label text="%Height" />
                        <TextField fx:id="dmHeightInput" prefWidth="60.0" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox fx:id="actionBox" alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" spacing="5.0">
               <children>
                  <Label maxWidth="1.7976931348623157E308" visible="false" HBox.hgrow="ALWAYS" />
                          <Button fx:id="startButton" mnemonicParsing="false" onAction="#startAction" onMouseEntered="#popSaveAs" styleClass="main-button" stylesheets="@../styles/MyBox.css" text="%Create" />
                  <Button fx:id="validateButton" layoutX="1716.0" layoutY="13.0" mnemonicParsing="false" onAction="#validateAction" onMouseEntered="#popSaveAs">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/iconView.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="saveButton1" mnemonicParsing="false" onAction="#saveAsAction" onMouseEntered="#popSaveAs">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/iconSave.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
                    <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true" VBox.vgrow="ALWAYS">
                        <content>
                            <AnchorPane fx:id="maskPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onMouseDragged="#showXY" onMouseMoved="#showXY" onMousePressed="#showXY" onMouseReleased="#showXY" VBox.vgrow="ALWAYS">
                                <children>
                                    <ImageView fx:id="imageView" cache="true" fitHeight="400.0" fitWidth="900.0" pickOnBounds="true" preserveRatio="true">
                                                </ImageView>
                                </children>
                            </AnchorPane>
                        </content>
                    </ScrollPane>
            </children>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
   <bottom>
      <Label fx:id="bottomLabel" BorderPane.alignment="CENTER_LEFT" />
   </bottom>
</BorderPane>
