<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="750.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImageManufactureFiltersController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" minHeight="120.0" VBox.vgrow="NEVER">
                    <children>
                        <TabPane fx:id="tabPane" maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" tabClosingPolicy="UNAVAILABLE" HBox.hgrow="ALWAYS">
                            <tabs>
                                <Tab fx:id="fileTab" text="%File" />
                                <Tab fx:id="sizeTab" text="%Size" />
                                <Tab fx:id="cropTab" text="%Crop" />
                                <Tab fx:id="colorTab" text="%Color" />
                                <Tab fx:id="effectsTab" text="%Effects" />
                                <Tab fx:id="filtersTab" text="%Filters">
                                    <content>
                                        <VBox alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT">
                                            <children>
                                                <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                                                    <children>
                                                        <ToolBar HBox.hgrow="ALWAYS">
                                                            <items>
                                                                <RadioButton mnemonicParsing="false" text=" " visible="false" />
                                                            </items>
                                                        </ToolBar>
                                                        <ToolBar maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="NEVER">
                                                            <items>
                                                                <RadioButton mnemonicParsing="false" selected="true" text="%Red">
                                                                    <toggleGroup>
                                                                        <ToggleGroup fx:id="filtersGroup" />
                                                                    </toggleGroup>
                                                                </RadioButton>
                                                                <RadioButton mnemonicParsing="false" text="%Yellow" toggleGroup="$filtersGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Green" toggleGroup="$filtersGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Cyan" toggleGroup="$filtersGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Blue" toggleGroup="$filtersGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Magenta" toggleGroup="$filtersGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Invert" toggleGroup="$filtersGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%RedInvert" toggleGroup="$filtersGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%GreenInvert" toggleGroup="$filtersGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%BlueInvert" toggleGroup="$filtersGroup" />
                                                                <RadioButton fx:id="grayRadio" mnemonicParsing="false" text="%Gray" toggleGroup="$filtersGroup" />
                                                                <RadioButton fx:id="bwRadio" mnemonicParsing="false" text="%BlackOrWhite" toggleGroup="$filtersGroup" />
                                                                <RadioButton mnemonicParsing="false" text="%Sepia" toggleGroup="$filtersGroup" />
                                                            </items>
                                                        </ToolBar>
                                                    </children>
                                                </HBox>
                                                <HBox alignment="CENTER_RIGHT">
                                                    <children>
                                                        <ToolBar HBox.hgrow="ALWAYS">
                                                            <items>
                                                                <Slider prefWidth="5.0" showTickLabels="true" showTickMarks="true" visible="false" />
                                                            </items>
                                                        </ToolBar>
                                                        <ToolBar HBox.hgrow="NEVER">
                                                            <items>
                                                                <Button fx:id="filtersScopeButton" mnemonicParsing="false" onAction="#setFiltersScope" text="%Scope..." />
                                                                <Button mnemonicParsing="false" onAction="#wholeFiltersScope" text="%WholeImage" />
                                                                <HBox fx:id="bwBox" alignment="CENTER_RIGHT" spacing="10.0">
                                                                    <children>
                                                                        <Label fx:id="scaleLabel" text="%Threshold" />
                                                                        <Slider fx:id="scaleSlider" blockIncrement="5.0" max="95.0" min="5.0" showTickLabels="true" showTickMarks="true" value="55.0" />
                                                                        <TextField fx:id="scaleInput" prefWidth="60.0" />
                                                                        <Label fx:id="unitLabel" text="\%" />
                                                                        <Button fx:id="binaryCalculateButton" mnemonicParsing="false" onAction="#calculateThreshold" text="%Calculate" />
                                                                    </children>
                                                                    <padding>
                                                                        <Insets left="20.0" right="20.0" />
                                                                    </padding>
                                                                </HBox>
                                                                <Button mnemonicParsing="false" onAction="#filtersAction" text="%OK" />
                                                            </items>
                                                        </ToolBar>
                                                    </children>
                                                </HBox>
                                            </children>
                                        </VBox>
                                    </content>
                                </Tab>
                                <Tab fx:id="convolutionTab" text="%Convolution" />
                                <Tab fx:id="replaceColorTab" text="%ReplaceColor" />
                                <Tab fx:id="watermarkTab" text="%Watermark" />
                                <Tab fx:id="coverTab" text="%Cover" />
                                <Tab fx:id="arcTab" text="%Arc" />
                                <Tab fx:id="shadowTab" text="%Shadow" />
                                <Tab fx:id="transformTab" text="%Transform" />
                                <Tab fx:id="marginsTab" text="%Margins" />
                                <Tab fx:id="viewTab" text="%View" />
                                <Tab fx:id="refTab" text="%Reference" />
                                <Tab fx:id="browseTab" text="%Browse" />
                            </tabs>
                        </TabPane>
                    </children>
                </HBox>
                <HBox fx:id="hotBox" alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT">
                    <children>
                        <ToolBar HBox.hgrow="ALWAYS">
                            <items>
                                <Label fx:id="tipsLabel" text="%Tips" />
                                <Button mnemonicParsing="false" visible="false" />
                            </items>
                        </ToolBar>
                        <ToolBar fx:id="hotBar" disable="true" maxWidth="1.7976931348623157E308" HBox.hgrow="NEVER">
                            <items>
                                <CheckBox fx:id="showRefCheck" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="%ShowReference" />
                                <CheckBox fx:id="showScopeCheck" disable="true" mnemonicParsing="false" selected="true" text="%ShowScope" />
                                <Button fx:id="outButton" mnemonicParsing="false" onAction="#zoomOut" text="%ZoomOut" />
                                <Button fx:id="inButton" mnemonicParsing="false" onAction="#zoomIn" text="%ZoomIn" />
                                <Button fx:id="wButton" mnemonicParsing="false" onAction="#paneSize" text="%PaneSize" />
                                <Button fx:id="oButton" mnemonicParsing="false" onAction="#imageSize" text="%ImageSize" />
                                <ComboBox fx:id="hisBox" prefWidth="150.0" />
                                <Button fx:id="undoButton" disable="true" mnemonicParsing="false" onAction="#undoAction" text="%Undo" />
                                <Button fx:id="redoButton" disable="true" mnemonicParsing="false" onAction="#redoAction" text="%Redo" />
                                <Button fx:id="recoverButton" disable="true" mnemonicParsing="false" onAction="#recovery" text="%RecoverCtrl" />
                                <Button fx:id="saveButton" disable="true" mnemonicParsing="false" onAction="#save" text="%SaveCtrl" />
                            </items>
                        </ToolBar>
                    </children>
                </HBox>
                <SplitPane fx:id="splitPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                    <items>
                        <VBox fx:id="imageBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <children>
                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="%CurrentImage" wrapText="true" VBox.vgrow="NEVER" />
                                <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true" VBox.vgrow="ALWAYS">
                                    <content>
                                        <ImageView fx:id="imageView" cache="true" fitHeight="546.0" fitWidth="1067.0" onMouseClicked="#clickImage" onMouseEntered="#setBottomLabel" pickOnBounds="true" preserveRatio="true">
                                            <cursor>
                                                <Cursor fx:constant="OPEN_HAND" />
                                            </cursor>
                                        </ImageView>
                                    </content>
                                </ScrollPane>
                            </children>
                        </VBox>
                    </items>
                </SplitPane>
            </children>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
        </VBox>
    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <bottom>
        <Label fx:id="bottomLabel" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER_LEFT" />
    </bottom>
</BorderPane>
