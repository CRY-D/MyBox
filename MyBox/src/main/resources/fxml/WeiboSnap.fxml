<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="680.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.WeiboSnapController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox fx:id="mainPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="548.0" prefWidth="1083.0" spacing="10.0">
            <children>
                <TitledPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%WebPageOptions">
                    <content>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" spacing="10.0" VBox.vgrow="NEVER">
                                    <children>
                                        <Label text="%WeiboAddress" />
                                        <TextField fx:id="addressInput" prefHeight="30.0" prefWidth="295.0" />
                                        <Label text="%WeiboAddressComments" />
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" spacing="10.0">
                                    <children>
                                        <Label text="%StartMonth" />
                                        <TextField fx:id="startInput" prefHeight="30.0" prefWidth="87.0" promptText="2016-05" />
                                        <Label text="%EmptyForVeryStart" />
                                        <Label text="%EndMonth">
                                            <padding>
                                                <Insets left="50.0" />
                                            </padding>
                                        </Label>
                                        <TextField fx:id="endInput" prefHeight="30.0" prefWidth="80.0" promptText="2018-09" />
                                        <Label text="%EmptyForNow" />
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" spacing="10.0">
                                    <children>
                                        <Label text="%WebPageScale" />
                                        <ComboBox fx:id="zoomBox" editable="true" prefWidth="80.0" />
                                        <Label text="%WebWidth">
                                            <padding>
                                                <Insets left="20.0" />
                                            </padding>
                                        </Label>
                                        <ComboBox fx:id="widthBox" editable="true" prefWidth="150.0" />
                                    </children>
                                </HBox>
                                <HBox fx:id="delayBox" alignment="CENTER_LEFT" spacing="10.0" VBox.vgrow="NEVER">
                                    <children>
                                        <Label text="%LoadRetryDelay" />
                                        <ComboBox fx:id="loadDelayBox" editable="true" prefWidth="100.0" />
                                        <Label text="%MaxLoadDelay">
                                            <padding>
                                                <Insets left="10.0" />
                                            </padding>
                                        </Label>
                                        <ComboBox fx:id="maxDelayBox" editable="true" prefWidth="120.0" />
                                        <Label text="%ScrollDelay">
                                            <padding>
                                                <Insets left="10.0" />
                                            </padding>
                                        </Label>
                                        <ComboBox fx:id="scrollDelayBox" editable="true" prefWidth="100.0" />
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </content>
                </TitledPane>
                <TitledPane text="%SnapOptions">
                    <content>
                        <VBox spacing="10.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" spacing="10.0">
                                    <children>
                                        <Label text="%OneImage" />
                                        <RadioButton mnemonicParsing="false" selected="true" text="%PerScreen">
                                            <toggleGroup>
                                                <ToggleGroup fx:id="imageGroup" />
                                            </toggleGroup>
                                        </RadioButton>
                                        <RadioButton mnemonicParsing="false" text="%PerPage" toggleGroup="$imageGroup" />
                                        <Label text="%NotRecommended" />
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" spacing="10.0" VBox.vgrow="NEVER">
                                    <children>
                                        <Label text="%Format" />
                                        <RadioButton mnemonicParsing="false" selected="true" text="%PNG">
                                            <toggleGroup>
                                                <ToggleGroup fx:id="formatGroup" />
                                            </toggleGroup>
                                        </RadioButton>
                                        <RadioButton mnemonicParsing="false" text="%JpegQuailty" toggleGroup="$formatGroup" />
                                        <ComboBox fx:id="jpegBox" editable="true" prefWidth="80.0" />
                                        <Label text="\%">
                                            <padding>
                                                <Insets right="10.0" />
                                            </padding>
                                        </Label>
                                        <RadioButton mnemonicParsing="false" text="%CCITT4" toggleGroup="$formatGroup" />
                                        <TextField fx:id="thresholdInput" prefWidth="60.0" />
                                        <Label text="\%" />
                                        <Label text="%EmptyForDefault" />
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </content>
                </TitledPane>
                <TitledPane maxWidth="1.7976931348623157E308" text="%PdfOptions" VBox.vgrow="NEVER">
                    <content>
                        <VBox spacing="10.0">
                            <children>
                                <HBox fx:id="sizeBox" alignment="CENTER_LEFT" spacing="10.0">
                                    <children>
                                        <Label text="%PageSize">
                                            <HBox.margin>
                                                <Insets />
                                            </HBox.margin>
                                        </Label>
                                        <RadioButton mnemonicParsing="false" selected="true" text="%ImagesSize">
                                            <toggleGroup>
                                                <ToggleGroup fx:id="sizeGroup" />
                                            </toggleGroup>
                                        </RadioButton>
                                        <RadioButton mnemonicParsing="false" text="%StandardSize" toggleGroup="$sizeGroup" />
                                        <ComboBox fx:id="standardSizeBox" disable="true" prefHeight="30.0" prefWidth="215.0" />
                                        <ComboBox fx:id="standardDpiBox" disable="true" prefWidth="120.0" />
                                        <RadioButton mnemonicParsing="false" text="%Custom" toggleGroup="$sizeGroup" />
                                        <Label text="%UnitPixels" />
                                        <TextField fx:id="customWidthInput" disable="true" prefWidth="70.0" promptText="%Width" />
                                        <TextField fx:id="customHeightInput" disable="true" prefWidth="70.0" promptText="%Height" />
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" spacing="10.0" VBox.vgrow="NEVER">
                                    <children>
                                        <Label text="%PageMargin">
                                            <HBox.margin>
                                                <Insets />
                                            </HBox.margin>
                                        </Label>
                                        <ComboBox fx:id="MarginsBox" editable="true" prefWidth="80.0" />
                                        <Label text="%Author">
                                            <padding>
                                                <Insets left="10.0" />
                                            </padding>
                                        </Label>
                                        <TextField fx:id="authorInput" prefHeight="30.0" prefWidth="325.0" promptText="Mara">
                                            <HBox.margin>
                                                <Insets />
                                            </HBox.margin>
                                        </TextField>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </content>
                </TitledPane>
                <TitledPane maxWidth="1.7976931348623157E308" text="%TargetFiles" VBox.vgrow="NEVER">
                    <content>
                        <VBox spacing="10.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" spacing="10.0">
                                    <children>
                                        <Label text="%FilesPath" />
                                        <TextField fx:id="pathInput" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                                        <Button mnemonicParsing="false" onAction="#selectPath" text="%Select..." />
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" spacing="10.0">
                                    <children>
                                        <CheckBox fx:id="htmlCheck" mnemonicParsing="false" selected="true" text="%SaveHtmlFiles" />
                                        <CheckBox fx:id="pdfCheck" mnemonicParsing="false" selected="true" text="%SaveSnapshotsPDF" />
                                        <CheckBox fx:id="keepPageCheck" mnemonicParsing="false" text="%KeepEachPagePdf" />
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </content>
                </TitledPane>
            </children>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
        </VBox>
    </center>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <bottom>
        <HBox alignment="CENTER_LEFT" nodeOrientation="RIGHT_TO_LEFT" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="startButton" mnemonicParsing="false" onAction="#startAction" text="%Start" />
                <Button fx:id="exampleButton" mnemonicParsing="false" onAction="#exampleAction" text="%WowAsExample" />
            <CheckBox fx:id="miaoCheck" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" selected="true" text="%meow" />
            </children>
            <BorderPane.margin>
                <Insets top="10.0" />
            </BorderPane.margin>
        </HBox>
    </bottom>
</BorderPane>
